{
    "name": "Docs Extension",
    "description": "api calls",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js", "popup.js", "/resources/jquery.min.js"]
        }
    ],
     "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
         "service_worker": "background.js" 
    },
    "web_accessible_resources": [{
        "resources": ["dialogBox.html", "popup.html"],
        "matches": ["<all_urls>"]
    }],
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "system.display",
        "alarms"
    ],
    "action": {},
    "commands": {
        "inject-script": {
          "suggested_key": "Ctrl+Shift+Q",
          "description": "Inject a script on the page"
        }
    }
}