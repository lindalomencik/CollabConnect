{
    "name": "Pop up turotial",
    "description": "following a tut",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js", "popup.js", "/resources/jquery.min.js"]
        }
    ],
    "background": {
        "service_worker":"background.js"
    },
    "web_accessible_resources": [{
        "resources": ["dialogBox.html", "popup.html"],
        "matches": ["<all_urls>"]
    }],
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "system.display"
    ],
    "action": {},
    "commands": {
        "inject-script": {
          "suggested_key": "Ctrl+Shift+Y",
          "description": "Inject a script on the page"
        }
    }
}